{
  "info": {
    "watch_type": "miband4",
    "description": "Medtronic 780G-tailored watchface",
    "author": "mortenfyhn",
    "version": "0.0"
  },
  "use_custom_arrows": true,
  "arrow_position": {
    "x": 83,
    "y": 112
  },
  "graph": {
    "position": {
      "x": 0,
      "y": 0
    },
    "width": 120,
    "height": 120,
    "in_range_line": {
      "color": "#ff1ba8c6",
      "has_lines": false,
      "line_thickness": 1,
      "has_points": true
    },
    "low_val_line": {
      "color": "#ff1ba8c6",
      "has_lines": false,
      "line_thickness": 1,
      "has_points": true
    },
    "high_val_line": {
      "color": "#ff1ba8c6",
      "has_lines": false,
      "line_thickness": 1,
      "has_points": true
    },
    "predictive_line": {
      "display": false,
      "color": "#ff00ff"
    },
    "poly_predictive_line": {
      "display": false,
      "color": "#00ffff"
    },
    "bg_color": "#000000"
  },
  "bg_value_text": {
    "color_high": "#ffffffff",
    "color_low": "#ffffffff",
    "position": {
      "x": 42,
      "y": 149
    },
    "text_pattern": "$value",
    "text_settings": {
      "font_family": "Roboto-Bold",
      "font_size": 50,
      "stroke_width": 2,
      "text_align": "right",
      "text_scale": 0.88,
      "text_style": "bold"
    }
  },
  "delta_time_text": {
    "position": {
      "x": 55,
      "y": 14
    },
    "text_pattern": "at $time",
    "text_settings": {
      "font_family": "Roboto-Bold",
      "font_size": 17
    }
  },
  "pump_iob_text": {
    "position": {
      "x": 0,
      "y": 32
    },
    "text_pattern": "$value active",
    "text_settings": {
      "font_family": "Roboto-Bold",
      "font_size": 17
    }
  },
  // "pump_reservoir_text": {
  //   "position": {
  //     "x": 0,
  //     "y": 108
  //    },
  //    "text_pattern": "$value in pump",
  //    "text_settings": {
  //       "font_family": "Roboto-Bold",
  //       "font_size": 17
  //    }
  // },
  "no_readings_text": {
    "position": {
      "x": 0,
      "y": 70
    },
    "text_pattern": "No data",
    "text_settings": {
      "font_family": "Roboto-Regular",
      "font_size": 30,
      "text_align": "center"
    }
  },
  "no_readings_time_text": {
    "position": {
      "x": 0,
      "y": 133
    },
    "text_pattern": "at $time",
    "text_settings": {
      "font_family": "Roboto-Regular",
      "font_size": 18,
      "text_align": "right"
    }
  }
}
